{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2022",
        "outDir": "out",
        "sourceMap": true,
        "strict": true,
        "rootDir": "src",
        "lib": ["ES2022"],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test",
        "out"
    ]
}